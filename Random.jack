class Random {
    static int prev;
    static int a;
    static int c;

    function int randInt(int bottomLimit, int topLimit) {
        var int range;
        
        if (prev = 0) {
            let prev = 1;
        }
        let a = 42;
        let c = 27;
        let range = topLimit - bottomLimit;
        let prev = Math2.mod(a * prev + c, range);
        
        return bottomLimit + prev;
    }
}